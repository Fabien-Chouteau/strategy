with "config/strategy_tests_config.gpr";
with "config/strategy_tests_list_config.gpr";
with "../strategy.gpr";

project Strategy_Tests is
   for Source_Dirs use ("src/", "common/", "config/", "one_of/");
   for Object_Dir use "obj/" & Strategy_Tests_Config.Build_Profile;
   for Create_Missing_Dirs use "True";
   for Exec_Dir use "bin";
   for Main use Strategy_Tests_List_Config.Test_Mains;

   package Compiler is
      for Default_Switches ("Ada") use Strategy_Tests_Config.Ada_Compiler_Switches;
   end Compiler;

   package Binder is
      for Switches ("Ada") use ("-Es"); --  Symbolic traceback
   end Binder;
end Strategy_Tests;
